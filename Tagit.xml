<Ui xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns="http://www.blizzard.com/wow/ui/">
	<Script file="Tagit.lua" />
	<Frame name="Tagit_Frame" hidden="true" parent="UIParent" toplevel="true">
		<Size>
			<AbsDimension x="200" y="200" />
		</Size>
		<Anchors>
			<Anchor point="TOPLEFT">
				<Offset>
					<AbsDimension x="100" y="-150" />
				</Offset>
			</Anchor>
		</Anchors>
		<Backdrop bgFile="" edgeFile="" tile="true">
			<BackgroundInsets>
				<AbsInset left="11" right="12" top="12" bottom="11" />
			</BackgroundInsets>
			<TileSize>
				<AbsValue val="32" />
			</TileSize>
			<EdgeSize>
				<AbsValue val="32" />
			</EdgeSize>
		</Backdrop>
		<Scripts>
			<OnLoad>
				Tagit.OnLoad(self)
			</OnLoad>
		</Scripts>
	</Frame>
</Ui>